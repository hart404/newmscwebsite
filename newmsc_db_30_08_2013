-- Definition of table `news_item`
--

DROP TABLE IF EXISTS `news_item`;
CREATE TABLE `news_item` (
  `id` bigint(20) NOT NULL AUTO_INCREMENT,
  `version` bigint(20) NOT NULL,
  `author_name` varchar(100) DEFAULT NULL,
  `blogurl` varchar(100) DEFAULT NULL,
  `date_created` datetime NOT NULL,
  `description_metadata` varchar(156) DEFAULT NULL,
  `display_end_date` date NOT NULL,
  `display_start_date` date NOT NULL,
  `important` bit(1) NOT NULL,
  `keywords_metadata` longtext,
  `last_updated` datetime NOT NULL,
  `more_information` longtext NOT NULL,
  `photo_id` bigint(20) DEFAULT NULL,
  `summary` longtext NOT NULL,
  `title` varchar(100) NOT NULL,
  PRIMARY KEY (`id`),
  KEY `FK11ED74FF54DB6083` (`photo_id`),
  CONSTRAINT `FK11ED74FF54DB6083` FOREIGN KEY (`photo_id`) REFERENCES `scmsphoto` (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=latin1;

