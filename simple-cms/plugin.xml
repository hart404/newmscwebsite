<plugin name='simple-cms' version='0.1' grailsVersion='2.0.4 &gt; *'>
  <author>Phil Hartley</author>
  <authorEmail>hart404@gmail.com</authorEmail>
  <title>Simple CMS Plugin</title>
  <description>Provides an easy to use Content Management System for Grails web sites.
</description>
  <documentation>http://grails.org/plugin/simple-cms</documentation>
  <type>SimpleCmsGrailsPlugin</type>
